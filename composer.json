{
  "name": "digitalcloud/nova-highcharts",
  "description": "A Laravel Nova metric.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "laravel/nova": "*"
  },
  "autoload": {
    "psr-4": {
      "Digitalcloud\\NovaHighcharts\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Digitalcloud\\NovaHighcharts\\MetricsServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
